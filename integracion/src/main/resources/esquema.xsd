<?xml version="1.0"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
    targetNamespace="https://t4is.uv.mx/saludos"
    xmlns:tns="https://t4is.uv.mx/saludos"
    elementFormDefault="qualified">
    <xs:element name="SucesorRequest">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="numero" type="xs:int" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="SucersorResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="resultado" type="xs:int" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>